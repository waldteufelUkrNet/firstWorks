      .logo_part
        |LOGOGO
      .central-part
        .central-part__account
          .central-part__account-type
            |Account:
            |
            span Demo
          include BEM-blocks/simple-switcher/simple-switcher.pug
        img.central-part__currency-img(src='img/Bitcoin.png' alt='img/Bitcoin.png')
        .central-part__currency-value 9999.99
        button.central-part__btn(title='пополнить счет')
          img(src='img/wallet.png' alt='wallet.png')
      .right-part
        button.call-us-btn(title='contact us'): img(src="img/call-center.png", alt="call-center.png")
        span Menu:
        include BEM-blocks/menu-btn/menu-btn.pug
        span Lang:
        include BEM-blocks/language-switcher/language-switcher.pug
